  <div class="form-actions">
  <% if presenter.solr_document._source["filetype_tesim"] %>
  
  
    <% if presenter.solr_document._source["filetype_tesim"][0]!='externalurl' %>
      <%= link_to "Download this File", main_app.download_path(presenter), class: 'btn btn-default', data: { turbolinks: false }, target: :_blank %>
      <% if can? :edit, presenter.id %>
        <%= link_to "Edit this File", edit_polymorphic_path([main_app, presenter]), class: 'btn btn-default'  %>
      <% end %>
      <%= link_to "Back to #{parent.human_readable_type}", parent_path(parent), class: 'btn btn-default' %>
    <% else
         externalurl = presenter.solr_document._source["external_file_url_tesim"][0]
    %>
        <%= link_to "View external file", externalurl, class: 'btn btn-default', data: { turbolinks: false }, target: :_blank %>
        <% if can? :edit, presenter.id %>
            <%= link_to "Edit external file", edit_polymorphic_path([main_app, presenter]), class: 'btn btn-default'  %>
        <% end %>
        <%= link_to "Back to #{parent.human_readable_type}", parent_path(parent), class: 'btn btn-default' %>
    <% end %>
	
	
  <% else %>
	<%= link_to "Download this File", main_app.download_path(presenter), class: 'btn btn-default', data: { turbolinks: false }, target: :_blank %>
    <% if can? :edit, presenter.id %>
      <%= link_to "Edit this File", edit_polymorphic_path([main_app, presenter]), class: 'btn btn-default'  %>
    <% end %>
  <% end  %>
  </div>
