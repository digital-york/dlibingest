<% open('/var/tmp/external.log', 'w') { |f| f << presenter.member_presenters.to_s }   %>

<% if presenter.member_presenters.present? and (presenter.member_presenters.to_s.include? '["externalurl"]')%>

<div class="panel panel-default related_files">
  <div class="panel-heading">
    <h2><%= t('curation_concerns.show.related_external_files.heading') %></h2>
  </div>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Thumbnail</th>
        <th>Url</th>
        <th>Date added</th>
        <th>Visibility</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: 'external_member', collection: presenter.member_presenters %>
    </tbody>
  </table>
</div>
<% elsif can? :edit, presenter.id %>
  <h2><%= t('curation_concerns.show.related_files.heading') %></h2>
  <p class="text-center"><em>This <%= presenter.human_readable_type %> has no members associated with it. You can add one using the "Attach a File" button below.</em></p>
<% end %>
